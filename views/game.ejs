<!DOCTYPE html>
<html>
	<head>
		<title><%= title %></title>
		<link rel='stylesheet' href='/stylesheets/style.css' />
	</head>
	<body>
		<h1><%= title %></h1>
		<div><a href="/">Home</a></div>


		<table border="1">
			<thead>
				<tr>
					<th>ID</th>
					<th>First Name</th>
					<th>Last Name</th>
				</tr>
			</thead>
			<tbody>
				<% playerList.forEach(attendee => { %>
					<tr>
						<td><%= attendee.attendeeSummary[0].userSummary.userID %></td>
						<td><%= attendee.attendeeSummary[0].userSummary.firstName %></td>
						<td><%= attendee.attendeeSummary[0].userSummary.lastName %></td>
					</tr>
				<% }) %>
			</tbody>
		</table>

		<pre><%= JSON.stringify(gameInfo) %></pre>
		<pre><%= JSON.stringify(playerList) %></pre>
	</body>
</html>
